include ../profile-item/profile-item

form.profile__container
  .profile__header
    .profile__avatar
  .profile__main
    +profileItem('Почта')
      | !{ email }
    +profileItem('Логин')
      | !{ login }
    +profileItem('Имя')
      | !{ firstName }
    +profileItem('Фамилия')
      | !{ secondName }
    +profileItem('Имя в чате')
      | !{ displayName }
    +profileItem('Телефон')
      | !{ phone }
  .profile__footer
    | !{ saveButton }
